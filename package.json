{
    "name": "itsme-client",
    "version": "1.0.1",
    "description": "A library to process itsme responses and consume itsme endpoints.",
    "es2015": "index.js",
    "main": "cjs/index.js",
    "module": "./_esm5/index.js",
    "types": "index.d.ts",
    "scripts": {
        "build": "npm-run-all clean_dist build_cjs build_esm5 build_esm2015 copy-before-publish",
        "build_cjs": "npm-run-all clean_dist_cjs && npm run compile_dist_cjs",
        "build_esm5": "npm-run-all clean_dist_esm5 compile_dist_esm5",
        "build_esm2015": "npm-run-all clean_dist_esm2015 compile_dist_esm2015",
        "clean_dist": "rimraf ./dist",
        "clean_dist_cjs": "rimraf ./dist/cjs",
        "clean_dist_esm5": "rimraf ./dist/esm5",
        "clean_dist_esm2015": "rimraf ./dist/esm2015",
        "compile_dist_cjs": "tsc -p ./tsconfig/tsconfig.cjs.json",
        "compile_dist_esm5": "tsc -p ./tsconfig/tsconfig.esm5.json",
        "compile_dist_esm2015": "tsc -p ./tsconfig/tsconfig.esm2015.json",
        "copy-before-publish": "cp -R ./package.json ./README.md ./@types ./dist",
        "lint": "eslint \"src/**/*.ts\""
    },
    "dependencies": {
        "axios": "^0.18.0",
        "base64url": "^3.0.0",
        "jwk-rsa-compute-primes": "^1.0.0",
        "lru-cache": "^4.1.3",
        "ms": "^2.1.1",
        "node-jose": "^1.0.0",
        "qs": "^6.5.2",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@matthiaskunnen/eslint-config-typescript-node": "^1.0.0",
        "@types/lru-cache": "^4.1.1",
        "@types/node": "^10.9.4",
        "husky": "^4.2.5",
        "lint-staged": "^10.2.11",
        "npm-run-all": "^4.1.3",
        "rimraf": "^2.6.2",
        "ts-node": "^7.0.1",
        "tslib": "^2.0.0",
        "typescript": "~3.9.7"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MatthiasKunnen/node-itsme-client.git"
    },
    "keywords": [
        "itsme",
        "client",
        "node"
    ],
    "engines": {
        "node": ">=12.0.0",
        "yarn": "~1.22.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "author": "Matthias Kunnen <matthias.kunnen@gmail.com>",
    "license": "LGPL-3.0-only",
    "bugs": {
        "url": "https://github.com/MatthiasKunnen/node-itsme-client/issues"
    },
    "homepage": "https://github.com/MatthiasKunnen/node-itsme-client#readme"
}
